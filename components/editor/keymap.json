    "close-tab": "ctrl+f4",
    "save": "ctrl+s",
    "save-as": "ctrl+shift+s",
    "toggle-menubar": "ctrl+k ctrl+m",
    "toggle-extension": "ctrl+k ctrl+e",
    "toggle-full-ext": "ctrl+k ctrl+f",
    "command-palette": "f1",
