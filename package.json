{
  "name": "thulium-music-3",
  "version": "3.0.0",
  "description": "Thulium Music 3",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack-win-x64": "electron-packager . \"Thulium Music 3\" --win --arch=x64 --out=./dist --app-version=3.0.0 --overwrite --icon=./assets/icon.ico",
    "pack-win-x86": "electron-packager . \"Thulium Music 3\" --win --arch=ia32 --out=./dist --app-version=3.0.0 --overwrite --icon=./assets/icon.ico",
    "publish": "electron-builder --publish onTagOrDraft"
  },
  "build": {
    "appId": "com.obstudio.thulium",
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "category": "public.app-category.music",
      "target": "dmg",
      "icon": "assets/icon.icns"
    }
  },
  "keywords": [],
  "author": "Ob Studio",
  "devDependencies": {
    "@vue/devtools": "^1.1.0",
    "electron": "^2.0.0",
    "electron-builder": "^20.16.1",
    "eslint": "^4.19.1"
  },
  "dependencies": {
    "acorn": "^5.7.1",
    "element-ui": "^2.4.1",
    "js-yaml": "^3.12.0",
    "monaco-editor": "^0.13.1",
    "mousetrap": "^1.6.2",
    "vue": "^2.5.16",
    "vue-i18n": "^7.8.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1",
    "wafd-tm3": "^1.0.1",
    "audiobuffer-to-wav": "^1.0.0"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obstudio/Thulium-Music-3.git"
  },
  "bugs": {
    "url": "https://github.com/obstudio/Thulium-Music-3/issues"
  },
  "homepage": "https://github.com/obstudio/Thulium-Music-3#readme"
}
